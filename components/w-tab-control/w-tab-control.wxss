/* components/w-tab-control/w-tab-control.wxss */
.tab-control{
  display: flex;
  margin-bottom: 16rpx;
  background: #fff;
  padding-bottom: 8rpx;
}
.tab-control-item{
  flex: 1;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
}
.active{
  color: #f55777;
}
.tab-control-item.active text{
  padding: 16rpx 10rpx;
  border-bottom: 6rpx solid #ff5777;
}